<root>
    <events>
        <event>
            <name>BathroomLightFlicker</name>
            <description>The bathroom lights flicker.</description>
            <consequence>
                <action>lightFlickering</action>
            </consequence>
        </event>
        <event>
            <name>RespawnNormal</name>
            <description>The player respawns in a normal bathroom.</description>
            <consequence>
                <action>respawnPlayerNormal</action>
            </consequence>
        </event>
        <event>
            <name>RespawnAltered</name>
            <description>The player respawns in an altered bathroom.</description>
            <consequence>
                <action>respawnPlayerAltered</action>
            </consequence>
        </event>
    </events>
</root>

